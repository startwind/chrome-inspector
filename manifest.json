{
  "manifest_version": 3,
  "name": "Startwind Inspector",
  "version": "1.1.0",
  "description": "Monitors requests, runs modular checks, and displays only findings for the active tab.",
  "permissions": [
    "storage",
    "webRequest",
    "tabs",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "devtools_page": "devtools.html"
}